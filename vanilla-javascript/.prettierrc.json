{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "charset": "utf-8",
  "insert_final_newline": true,
  "end_of_line": "lf",
  "indent_style": "space",
  "indent_size": 2,
  "max_line_length": 80
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "indent_style": "space"
      }
    }
  ]
}
